[INDIVIDUAL]
input = {Cl_pop, omega_Cl, V_pop, omega_V}

DEFINITION:
Cl = {distribution=logNormal, typical=Cl_pop, sd=omega_Cl}
V = {distribution=logNormal, typical=V_pop, sd=omega_V}

[LONGITUDINAL]
input = {Cl, V, a}

EQUATION:
Cc = pkmodel(V, Cl)

DEFINITION:
y = {distribution=logNormal, prediction=Cc, errorModel=constant(a)}

OUTPUT:
output = {y, Cc}
